{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PipelineOptions": {
    "Elements": [
      {
        "BuilderName": "CloudRequestEngineBuilder",
        // Obtain a resource key with the properties required to test this 
        // example for free: https://configure.51degrees.com       
        // The properties used in this example are:
        // IpRangeStart,IpRangeEnd,CountryCode,
        // CityName,AverageLocation,LocationBoundNorthWest,
        // LocationBoundSouthEast
        "BuildParameters": {
          "ResourceKey": "!!ENTER_YOUR_RESOURCE_KEY_HERE!!"
        }
      },
      {
        "BuilderName": "IpiCloudEngineBuilder"
      }
    ]
  },
  "PipelineWebIntegrationOptions": {
    // Both these options default to true anyway.
    // They are specified here for illustrative purposes.
    "ClientSideEvidenceEnabled": "true",
    "UseAsyncScript": "true"
  }
}
